<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="module_category_bank_sync" model="ir.module.category">
        <field name="name">Synchronisation Bancaire</field>
        <field name="sequence">30</field>
    </record>

    <record id="group_bank_sync_user" model="res.groups">
        <field name="name">Utilisateur</field>
        <field name="category_id" ref="module_category_bank_sync"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_bank_sync_manager" model="res.groups">
        <field name="name">Gestionnaire</field>
        <field name="category_id" ref="module_category_bank_sync"/>
        <field name="implied_ids" eval="[(4, ref('group_bank_sync_user'))]"/>
    </record>
</odoo>
